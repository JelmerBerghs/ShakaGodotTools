{
  "name": "GodotTools",
  "displayName": "Shaka: Godot Tools",
  "description": "VSCode tools for godot",
  "publisher": "ShakaRule",
  "icon": "./icon.png",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/JelmerBerghs/godotsceneselector.git"
  },
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "GodotTools.SelectScene",
        "title": "Godot Tools: Select scene",
        "icon": "$(debug-alt)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  }
}
